	// look for documentation in package body
	function xmlEncodeSingleElement(elem){

		var i;
		var xmlString = "";

		if(elem.type){
			if(elem.name){
				if(elem.type=="text" || elem.type=="textarea" || elem.type=="select-one" ||
					elem.type=="hidden" || elem.type=="password" ){

					if(elem.value){
						xmlString="<"+elem.name+">"+xmlEncode(elem.value)+"</"+elem.name+">";
					}else{
						xmlString="<"+elem.name+"></"+elem.name+">";
					}
				}else if (elem.type=="radio" || elem.type=="checkbox"){
					if(elem.value && elem.checked){
						xmlString="<"+elem.name+">"+xmlEncode(elem.value)+"</"+elem.name+">";
					}
				}else if (elem.type=="select-multiple"){
					if (elem.options) {
						for(i=0; i<elem.options.length; i++){
							if(elem.options[i].selected){
								xmlString+="<"+elem.name+">"+xmlEncode(elem.options[i].value)+"</"+elem.name+">";
							}
						}
					}
				}
			}
		}
		return(xmlString);
	}

	// look for documentation in package body
	function xmlEncodeFormElements(d){

		var xmlString = "";
		var div;
		var i;
		var elem

		if ((typeof d) == "string"){
			div=document.getElementById(d);
		}else if(((typeof d) == "object")&&(d.getElementsByTagName)){
			div=d;
		}

		if (div){
			if (div.elements){
				// the form

				for(i=0; i<div.elements.length; i++){
					elem=div.elements[i];

					if (elem.type&&elem.name){
						// to exclude fieldset and label tags

						xmlString += xmlEncodeSingleElement(elem);
					}
				}
			}else if(div.getElementsByTagName){
				// a div or a table
				var formSelect=div.getElementsByTagName("select");
				for(i=0; i<formSelect.length; i++){
					elem=formSelect[i];
					if (elem.type&&elem.name){
						xmlString += xmlEncodeSingleElement(elem);
					}
				}

				var formInput=div.getElementsByTagName("input");
				for(i=0; i<formInput.length; i++){
					elem=formInput[i];
					if (elem.type&&elem.name){
						xmlString += xmlEncodeSingleElement(elem);
					}
				}

				var textArea=div.getElementsByTagName("textarea");
				for(i=0; i<textArea.length; i++){
					elem=textArea[i];
					if (elem.type&&elem.name){
						xmlString += xmlEncodeSingleElement(elem);
					}
				}
			} // form or div
		}else{
			// debugging
			alert( "xmlEncodeFormElements() div not found: " + d );
		}
		return xmlString;
	}

