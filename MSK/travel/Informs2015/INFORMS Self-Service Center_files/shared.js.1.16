	function encodeDateMMDDYYYY( dateArg ) {
		// adapted from Ext.util.JSON.encode / encodeDate

		return  pad(dateArg.getMonth() + 1) + "/" +
			pad(dateArg.getDate()) + "/" +
			dateArg.getFullYear();
	};

	function encodeDateMMDDYYYYHH24MISS( dateArg ) {
		// adapted from Ext.util.JSON.encode / encodeDate

		return  pad(dateArg.getMonth() + 1) + "/" +
			pad(dateArg.getDate()) + "/" +
			dateArg.getFullYear() + " " +
			pad(dateArg.getHours()) + ":" +
			pad(dateArg.getMinutes()) + ":" +
			pad(dateArg.getSeconds());
	};

	function getElementClass(e){
		if(e&&e.getAttribute){
			// try IE naming first
			var className = e.getAttribute("className");
			if (className){
				return className;
			}else{
				// then try FF
				className = e.getAttribute("class");
				if (className){
					return className;
				}
			}
		}
	}

	// overwrites any existing class(es)
	function setElementClass(e,className){
		if(e&&e.setAttribute){
			// try IE naming first
			if(!e.setAttribute("className",className)){
				// hopefully FF syntax works
				e.setAttribute("class",className);
			}
		}

	}

	// the array is an array of the same type as the search key
	function findInArray(arrayParm,searchFor){
		var idx=-1;

		for(var i=0; (i<arrayParm.length)&&(idx<0); i++){
			if(arrayParm[i]==searchFor){
				idx=i;
			}
		}

		if(idx>-1){
			return idx;
		}
	}

	function addClass(e,className){
		if (e){
			var classStr=getElementClass(e);
			if (classStr){
				var classArray=classStr.split(" ");

				if (!(findInArray(classArray,className)>=0)){
					classArray[classArray.length]=className;
					setElementClass(e,classArray.join(" "));
				}
			}else{
					setElementClass(e,className);
			}
		}else{
			// debugging
			alert( "addClass() calling error: no valid element passed in" )
		}
	}

	function removeClass(e,className){
		if (e){
			var classStr=getElementClass(e);
			if (classStr){
				var classArray=classStr.split(" ");
				var classIdx=findInArray(classArray,className);
				if (classIdx>=0){
					classArray.splice(classIdx,1);
					setElementClass(e,classArray.join(" "));
				}
			}
		}else{
			// debugging
			alert( "removeClass() calling error: no valid element passed in" )
		}
	}

	function replaceClass(e,oldClassName,newClassName){
		if (e){
			var classArray=getElementClass(e).split(" ");
			var classIdx=findInArray(classArray,oldClassName);
			if (!(classIdx>=0)){
				classIdx=classArray.length;
			}
			if (findInArray(classArray,newClassName)>=0){
				classArray.splice(classIdx,1);
			}else{
				classArray[classIdx]=newClassName;
			}
			setElementClass(e,classArray.join(" "));
		}else{
			// debugging
			alert( "replaceClass() calling error: no valid element passed in" )
		}
	}

