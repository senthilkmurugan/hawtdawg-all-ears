	function sharedPopup( urlParm, titleParm, attributesParm ){

		var popupWindow = open( urlParm, titleParm, attributesParm );

		if ( popupWindow.opener === null ) {
			popupWindow.opener = self;
		}
		popupWindow.focus();
		return false;
	}


