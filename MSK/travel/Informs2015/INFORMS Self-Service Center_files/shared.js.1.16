   	function xmlEncode(textToEncode){
		var result = textToEncode;

		var amp = /&/gi;
		var gt = />/gi;
		var lt = /</gi;
		var quot = /"/gi;
		var apos = /'/gi;

		var xml_amp = "&#38;";
		var xml_quot = "&#34;";
		var xml_gt = "&#62;";
		var xml_lt = "&#60;";
		var xml_apos = "&#39;";

		if (result&&result.replace&&typeof result.replace === "function") {
			result = result.replace(amp, xml_amp);
			result = result.replace(quot, xml_quot);
			result = result.replace(lt, xml_lt);
			result = result.replace(gt, xml_gt);
			result = result.replace(apos, xml_apos);
		}

		return result;
	};

	function isArray(value) { // from "JavaScript: The Good Parts" Chapter 6: Arrays / Section "Confusion" p. 61
