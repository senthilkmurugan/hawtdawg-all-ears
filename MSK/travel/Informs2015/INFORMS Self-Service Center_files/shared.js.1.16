// Begin Validation Functions
function aaTrim(str) {
    str = str.replace(/^\s*/, '').replace(/\s*$/, '');
    return str;
}

function aaIsNull( field ) {
  selected = 0;
  fieldIsNull = 0;
  if ( field.type == "text" || field.type == "password" ||
       field.type == "textarea" || field.type == "file" ) {
    if ( aaTrim(field.value) == "" )
      fieldIsNull = 1;
  } else if ( field.type == "select-one" ) {
      if (( aaTrim(field.options[field.selectedIndex].value) == "") ||
         ( field.options[field.selectedIndex].value == "%null%"))
        fieldIsNull = 1;
  } else if ( field.type == "select-multiple" ) {
      fieldIsNull = 1;
      for ( i = 0; i < field.length; i++ )
        if ( field.options[i].selected )
          fieldIsNull = 0;
  } else if ( field.type == "undefined" || field.type == "checkbox"  ||
              field.type == "radio" ) {
      fieldIsNull = 1;
      for ( i = 0; i < field.length; i++ )
        if ( field[i].checked )
          fieldIsNull = 0;
  }
  if ( fieldIsNull ) {
     return true;
  }
  else {
     return false;
  }
}
function aaIsNullArray( formElements ) {
  var selected = 0;
  if (typeof formElements[0] == 'undefined') // not array - possibly single value selected
  {
     if (formElements.checked==false || formElements.disabled==true )
        return true;
  }
  else
  {
     for (var i=0;i<formElements.length;i++)
     {
        if (formElements[i].checked==true && formElements[i].disabled==false )
          selected = 1;
     }
     if ( selected == 0 )
     {
        return true;
     }
  }
  return false;
}

function aaIsNumber(value)
{
  s = value;
  if (isNaN(Math.abs(value)) && (s.charAt(0) != '#'))
  {
         for (var i=0; (i <= s.length && s.charAt(i) != '.'); )
         {
          if (((s.charAt(i) >= 0) && (s.charAt(i) <= 9)) ||
               (s.charAt(i) == ',' && i != 0 && i != s.length-1) || (s.charAt(i) == '.') )
                 i++; 
          else 
           { 
             return false; 
           } 
         } 
        if (s.charAt(i) == '.') 
         { 
	     for (i++;i <= s.length; ) 
           { 
            if (((s.charAt(i) >= 0) && (s.charAt(i) <= 9))) 
              i++; 
            else 
            {
             return false;
            } 
           } 
         }
  } 
  return true;
}

function aaLongerThan( value, limit ) {
   return $.trim(value).length > limit;
}

function aaShorterThan( value, limit ) {
   return $.trim(value).length < limit;
}

function aaIsLowerCase( value ) {
   return (value == value.toLowerCase());
}

function aaIsUpperCase( value ) {
   return (value == value.toUpperCase());
}

// email regex changed by pn 20140512 eng-11898 as the original would allow anything after the email address 

function aaIsType(type, value)
{
  UpperType = type.toUpperCase();
  if (["INTEGER","NUMBER"].indexOf(UpperType) >= 0)
     var str = Math.abs(value);
  else var str = value;

  if (UpperType === "INTEGER")
     var filter=/(^\d+$)/;
  else if (UpperType === "NUMBER")
     return aaIsNumber(value);
  else if (UpperType === "ALPHA")
     var filter=/^[a-zA-Z]{1,}$/;
  else if (UpperType === "ALPHANUMERIC")
     var filter=/^[a-zA-Z0-9]{1,}$/;
  else if (UpperType === "EMAIL")
     var filter = /^[a-zA-Z0-9!#$%&'*+\/=?^_`{|}~-]+(?:\.[a-zA-Z0-9!#$%&'*+\/=?^_`{|}~-]+)*@(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?\.)+[a-zA-Z0-9](?:[a-zA-Z0-9-]*[a-zA-Z0-9])?$/; 
  else if (UpperType === "PHONE") // minimum of 7 digits - standard characters supported
     var filter = /^([a-zA-Z,#/ \.\(\)\-\+\*]*[0-9]){7}[0-9a-zA-Z,#/ \.\(\)\-\+\*]*$/;
  else alert("unsupported validation type [" + type + "]");

  if (str.length === 0)
     return true;
  else if (filter.test(str))  
     return true;
  else  
     return false;
}
