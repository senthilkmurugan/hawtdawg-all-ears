function HTMLtoPlaintext(HTML,dumpIframeId) {
      $("#" + dumpIframeId).empty();
      $("#" + dumpIframeId).append(HTML);
      $("#" + dumpIframeId + " style").remove();
      $("#" + dumpIframeId + " script").remove();
      var convertedText = $("#" + dumpIframeId).text();
      $("#" + dumpIframeId).empty();
      convertedText = $.trim(convertedText); // trims leading and trailing space
      convertedText = convertedText.replace(/\u00a0/g, " "); // this is the character created by &nbsp;
      return convertedText;
   }

